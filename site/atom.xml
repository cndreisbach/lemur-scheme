layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Lemur Scheme</title>
 <link href="http://crnixon.github.com/lemur-scheme/atom.xml" rel="self"/>
 <link href="http://crnixon.github.com/lemur-scheme/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://crnixon.github.com/lemur-scheme/</id>
 <author>
   <name>Clinton R. Nixon</name>
   <email>crnixon@gmail.com</email>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://crnixon.github.com/lemur-scheme{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://crnixon.github.com/lemur-scheme{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>